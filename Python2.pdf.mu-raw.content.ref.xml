

<w:p>
<w:r><w:rPr><w:rFonts w:ascii="OpenSans" w:hAnsi="OpenSans"/><w:sz w:val="20.000000"/><w:szCs w:val="15.000000"/></w:rPr><w:t xml:space="preserve"></w:t></w:r>
</w:p>

<w:p>
<w:r><w:rPr><w:rFonts w:ascii="CourierNewPSMT" w:hAnsi="CourierNewPSMT"/><w:sz w:val="20.000000"/><w:szCs w:val="15.000000"/></w:rPr><w:t xml:space="preserve">There was one who was famed for the number of things </w:t></w:r>
<w:r><w:rPr><w:rFonts w:ascii="CourierNewPSMT" w:hAnsi="CourierNewPSMT"/><w:sz w:val="20.000000"/><w:szCs w:val="15.000000"/></w:rPr><w:t xml:space="preserve">   He </w:t></w:r>
