</w:p>

<w:p>
<w:r><w:rPr><w:rFonts w:ascii="Helvetica-Bold" w:hAnsi="Helvetica-Bold"/><w:b/><w:sz w:val="21.900000"/><w:szCs w:val="16.425000"/></w:rPr><w:t xml:space="preserve">SYNOPSIS </w:t></w:r>
<w:r><w:rPr><w:rFonts w:ascii="Helvetica" w:hAnsi="Helvetica"/><w:sz w:val="20.000000"/><w:szCs w:val="15.000000"/></w:rPr><w:t xml:space="preserve">[see </w:t></w:r>
